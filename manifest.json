{
  "manifest_version": 2,
  "name": "Tab Manager",
  "version": "1.1",
  "description": "Manage tabs by tracking when you last access them.",
  "icons": { 
    "128": "icon128.png"
  },
  "incognito": "split",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon128.png"
  },
  "background": {
    "scripts": ["jquery-3.5.0.min.js", "background.js"],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}